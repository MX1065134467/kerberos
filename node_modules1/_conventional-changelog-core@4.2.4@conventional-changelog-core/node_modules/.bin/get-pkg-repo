#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_get-pkg-repo@4.2.1@get-pkg-repo/src/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_get-pkg-repo@4.2.1@get-pkg-repo/src/cli.js" "$@"
  ret=$?
fi
exit $ret
