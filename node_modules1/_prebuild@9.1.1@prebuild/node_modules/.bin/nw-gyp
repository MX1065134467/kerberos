#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_nw-gyp@3.6.5@nw-gyp/bin/nw-gyp.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_nw-gyp@3.6.5@nw-gyp/bin/nw-gyp.js" "$@"
  ret=$?
fi
exit $ret
