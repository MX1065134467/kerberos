#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_cmake-js@5.2.0@cmake-js/bin/cmake-js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_cmake-js@5.2.0@cmake-js/bin/cmake-js" "$@"
  ret=$?
fi
exit $ret
