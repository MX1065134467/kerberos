@ECHO off
SETLOCAL
CALL :find_dp0

SET _maybeQuote="
IF EXIST "%dp0%\python.exe" (
  SET "_prog=%dp0%\python.exe"
) ELSE (
  SET _maybeQuote=
  SET "_prog=python"
  SET PATHEXT=%PATHEXT:;.JS;=;%
)

%_maybeQuote%%_prog%%_maybeQuote%  "%dp0%\..\_clang-format@1.6.0@clang-format\bin\git-clang-format" %*
ENDLOCAL
EXIT /b %errorlevel%
:find_dp0
SET dp0=%~dp0
EXIT /b
