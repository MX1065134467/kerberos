#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_jsdoc-to-markdown@4.0.1@jsdoc-to-markdown/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_jsdoc-to-markdown@4.0.1@jsdoc-to-markdown/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
